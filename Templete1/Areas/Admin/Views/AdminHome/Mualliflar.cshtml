@model List<Muallif>
<!--app-content open-->
<div class="main-content app-content mt-0">
    <div class="side-app">

        <!-- CONTAINER -->
        <div class="main-container container-fluid">

            <!-- PAGE-HEADER -->
            <div class="page-header">
                <h1 class="page-title">Mualliflar</h1>

            </div>
            <div class="row row-sm">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-header">
                            <a asp-controller="AdminHome" asp-action="AddMualliflar">
                                <button class="btn btn-primary"> Qo'shish</button>
                            </a>

                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table id="file-datatable" class="table table-bordered text-nowrap key-buttons border-bottom">
                                    <thead>
                                        <tr>
                                            <th class="border-bottom-0">Id</th>
                                            <th class="border-bottom-0">Ism</th>
                                            <th class="bi-border-bottom-0">Familiya</th>
                                            <th class="border-bottom-0">Sharif</th>
                                            <th class="border-bottom-0">Ish joyi</th>
                                            <th class="border-bottom-0">Davlat</th>
                                            <th class="border-bottom-0">Ilmiy daraja</th>
                                            <th class="border-bottom-0">Ilmiy unvon</th>
                                            <th class="border-bottom-0">Elektron pochta</th>
                                            <th class="border-bottom-0">Telefon raqami</th>
                                            <th class="border-bottom-0">Registratsiya vaqti</th>
                                            <th class="border-bottom-0">O`zgartirish</th>
                                            <th class="border-bottom-0">O`chirish</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach(var i in Model)
                                        {
                                            <tr>
                                                <td>@i.Id</td>
                                                <td>@i.Ism</td>
                                                <td>@i.Familiya</td>
                                                <td>@i.Sharif</td>
                                                <td>@i.IshJoyi</td>
                                                <td>@i.Davlat</td>
                                                <td>@i.IlmiyDaraja</td>
                                                <td>@i.IlmiyUnvon</td>
                                                <td>@i.Email</td>
                                                <td>@i.Telefon</td>
                                                <td>@i.RegistratsiyaVaqti</td>
                                                <td><a class="btn btn-warning" asp-controller="AdminHome" asp-action="EditMuallif" asp-route-id="@i.Id">O`zgartirish</a></td>
                                                <td><a class="btn btn-danger" asp-controller="AdminHome" asp-action="DeleteMuallif" asp-route-id="@i.Id">O`chirish</a></td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- PAGE-HEADER END -->
            <!-- ROW OPEN -->
            <!-- ROW CLOSED -->
            <!-- ROW OPEN -->
            <!-- ROW CLOSED -->
            <!-- ROW OPEN -->
            <!-- ROW END -->
            <!-- ROW OPEN -->
            <!-- ROW CLOSED -->
            <!-- ROW OPEN -->
            <!-- ROW CLOSED -->
            <!-- ROW OPEN -->
            <!-- ROW CLOSED -->
            <!-- ROW OPEN -->
            <!-- ROW CLOSED -->
            <!-- ROW OPEN -->
            <!-- ROW CLOSED -->
            <!-- ROW OPEN -->
            <!-- ROW CLOSED -->
        </div>
        <!-- CONTAINER CLOSED -->
    </div>
</div>
<!--app-content closed-->
